<h2>Import New Playlist</h2>

<%= form_tag "/admin/playlists" do%>
  <p><%= label_tag :playlist_id %>
  <%= text_field_tag :playlist_id %></p>
  <p><%= label_tag :title %>
  <%= text_field_tag :title %></p>
  <p><%= label_tag :description %>
  <%= text_field_tag :description %></p>
  <p><%= label_tag :thumbnail %>
  <%= text_field_tag :thumbnail %></p>

  <%= submit_tag "Save", class: "mt2 btn btn-primary mb1 bg-teal"  %>
<% end %>
